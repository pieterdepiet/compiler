fun start(_ argc : Int) {
    if (argc < 2) {
        prints("No args");
    } else if (argc < 3) {
        prints("One arg");
    } else if (argc < 4) {
        prints("Two args");
    } else {
        if (argc < 5) {
            prints("Three args");
        } else {
            prints("More than three args");
        }
    }
}